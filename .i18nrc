{
  "plugins": [
    "@instructure/i18nliner-handlebars"
  ],

  "files": [
    {
      "pattern": "app/jsx/**/*.{coffee,js}",
      "processor": "js"
    },
    {
      "pattern": "app/views/jst/**/*.handlebars",
      "processor": "hbs"
    }
  ],

  "include": [
    "app/coffeescripts/.i18nrc",
    "client_apps/canvas_quizzes/.i18nrc",
    "gems/plugins/.i18nrc",
    "public/javascripts/.i18nrc"
  ]
}