<div class="outcomes-browser content-box">

  {{#unless skipToolbar}}
  <div class="toolbar outcomes-toolbar">
    <a href="#" class="go_back ui-button ui-widget ui-state-default ui-corner-all" role="button" style="display:none">
      {{#t}}Back{{/t}}
    </a>

    {{#if canManageOutcomes}}
    <button class="add_outcome_link ui-button ui-widget ui-state-default ui-corner-all">
      <i class="icon-plus"></i>
      <span class="ui-button-text" aria-hidden="true">{{#t}}Outcome{{/t}}</span>
      <span class="screenreader-only">{{#t}}Add a New Outcome{{/t}}</span>
    </button>
    <button class="add_outcome_group ui-button ui-widget ui-state-default ui-corner-all">
      <i class="icon-plus"></i>
      <span class="ui-button-text" aria-hidden="true">{{#t}}Group{{/t}}</span>
      <span class="screenreader-only">{{#t}}Add a New Outcome Group{{/t}}</span>
    </button>
    <button style="display:none" class="import_outcomes ui-button ui-widget ui-state-default ui-corner-all">
      <i class="icon-upload"></i>
      <span class="ui-button-text" aria-hidden="true">{{#t}}Import{{/t}}</span>
      <span class="screenreader-only">{{#t}}Import a set of Outcomes{{/t}}</span>
    </button>
    <button class="find_outcome ui-button ui-widget ui-state-default ui-corner-all">
      <i class="icon-search"></i>
      <span class="ui-button-text" aria-hidden="true">{{#t}}Find{{/t}}</span>
      <span class="screenreader-only">{{#t}}Find an Outcome{{/t}}</span>
    </button>
    {{/if}}
    <span id="popoverMenu" class="popover-menu"></span>
  </div>
  {{/unless}}

  <div class="outcomes-main">
    <div class="outcomes-sidebar">
      <div class="wrapper"></div>
    </div>
    <div class="outcomes-content">
    </div>
  </div>
</div>
