{
  "name": "tinymce-a11y",
  "version": "1.0.0",
  "description": "An accessibility checker plugin for TinyMCE.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --open",
    "extract": "format-message extract \"src/**/*.js\" > locales/en.json",
    "fmt": "prettier --no-semi --write \"src/**/*.js\"",
    "fmt-check": "prettier --no-semi -l \"src/**/*.js\""
  },
  "keywords": ["tinymce", "accessibility", "plugin", "a11y"],
  "author": "Brent Burgoyne",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "canvas-rce": "^2.2.1",
    "format-message-cli": "^5.2.0",
    "jest": "^20.0.4",
    "prettier": "^1.7.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "format-message": "^5.2.0",
    "instructure-icons": "^4.2.0",
    "instructure-ui": "^2.5.1",
    "react": "^15.6.1",
    "react-color": "^2.13.4",
    "react-dom": "^15.6.1",
    "wcag-element-contrast": "^1.0.1"
  }
}
