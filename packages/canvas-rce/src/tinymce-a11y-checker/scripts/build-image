#!/usr/bin/env bash

docker build \
  -t tinymce-a11y-checker \
  --build-arg NPM_PRIVATE_SCOPE=$NPM_PRIVATE_SCOPE \
  --build-arg NPM_PRIVATE_REGISTRY=$NPM_PRIVATE_REGISTRY \
  --build-arg NPM_PRIVATE_USERNAME=$NPM_PRIVATE_USERNAME \
  --build-arg NPM_PRIVATE_PASSWORD=$NPM_PRIVATE_PASSWORD \
  --build-arg NPM_PRIVATE_EMAIL=$NPM_PRIVATE_EMAIL \
  $1