<div title="{{#t}}Sync Grades{{/t}}" style="display:none;">
  <div class="before_external_content_info_alert" tabindex="0">
    <div class="screenreader-only">
      <div class="ic-flash-info">
        <div class="ic-flash__icon" aria-hidden="true">
          <i class="icon-info"></i>
        </div>
        {{t 'The following content is partner provided'}}
      </div>
    </div>
  </div>
  <iframe
    class="post-grades-frame"
    style="border: none; width: 100%; height: 100%;"
    tabindex="0"
    title="{{#t}}Sync Grades{{/t}}"
    allow="{{allowances}}"
    data-lti-launch="true"
  >
  </iframe>
  {{#if shouldRenderForwardingIframe}}
  <iframe
    src="/post_message_forwarding"
    id="post_message_forwarding"
    name="post_message_forwarding"
    title="post_message_forwarding"
    sandbox="allow-scripts allow-same-origin"
    style="display: none;"
  />
  {{/if}}
  <div class="after_external_content_info_alert" tabindex="0">
    <div class="screenreader-only">
      <div class="ic-flash-info">
        <div class="ic-flash__icon" aria-hidden="true">
          <i class="icon-info"></i>
        </div>
        {{t 'The preceding content is partner provided'}}
      </div>
    </div>
  </div>
</div>
